<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="data"
            type="com.example.shubufei.bean.Data" />
    </data>
<LinearLayout
    android:id="@+id/ll_layout"
    android:orientation="horizontal"
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="40dp">

        <TextView
            android:text="@{data.cinvcode}"
            style="@style/tv"
            android:layout_weight="9"
            />


        <TextView
            android:text="@{data.dstartdate}"
            style="@style/tv" />
        <TextView
            android:text="@{data.denddate}"
            style="@style/tv" />

        <TextView
            android:text="@{data.iquantity}"
            android:layout_weight="11"
            style="@style/tv" />

        <TextView
            android:text="@{data.idayqty}"
            android:layout_weight="11"
            style="@style/tv" />





</LinearLayout>
</layout>